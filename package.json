{
  "name": "my-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@10.17.0",
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=10.17.0"
  },
  "scripts": {
    "dev": "vite --mode dev --host",
    "build:dev": "vite build --mode  dev",
    "preview": "yarn run build && vite preview ",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "vue": "3.5.21",
    "vue-router": "4.4.5"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "svg-baker",
        "postcss"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^15.14.9",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "sass": "^1.90.0",
    "tailwindcss": "3.4.17",
    "typescript": "~5.8.3",
    "vite": "5.4.10",
    "vue-tsc": "2.1.6"
  }
}
